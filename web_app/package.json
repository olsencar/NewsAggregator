{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "firebase": "^7.14.0",
    "minimist": "^1.2.5",
    "mongoose": "^5.9.4",
    "node-cache": "^5.1.0",
    "recompose": "^0.30.0"
  },
  "scripts": {
    "start": "npm install && node server.js",
    "server": "nodemon server.js",
    "build": "npm run build --prefix client",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "client": "npm run start --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "engines": {
    "node": "10.16.0"
  }
}
